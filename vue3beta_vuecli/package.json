{
  "name": "vue3betavuecli",
  "version": "0.1.0",
  "private": true,
  "main": "dist/csii-vx-mobile.umd.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest",
    "build:@csii/vx-mobile": "vue-cli-service build --target lib --name csii-vx-mobile ./src/components/index.js",
    "build:@csii/vx-mobile-wc": "vue-cli-service build --target wc --name csii-vx-mobile ./src/components/index.js",
    "build:lib": "npm run build:@csii/vx-mobile",
    "build:wc": "npm run build:@csii/vx-mobile-wc",
    "clean": "rimraf ./lib",
    "output": "vue inspect > output.js",
    "prepare": "npm run build:@csii/vx-mobile"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0-0",
    "vue-router": "^4.0.0-beta.7",
    "vuex": "^4.0.0-beta.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "^4.5.4",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/server-test-utils": "^1.0.4",
    "@vue/test-utils": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "jest": "^26.4.2",
    "postcss-pxtorem": "^5.1.1",
    "sinon": "^9.0.3",
    "style-resources-loader": "^1.3.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-style-resources-loader": "~0.1.4",
    "webpack-cli": "^3.3.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": "off",
      "no-debugger": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
